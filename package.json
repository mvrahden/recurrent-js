    "neural network",
    "artificial neural network",
    "deep neural network",
    "feedforward neural network",
    "lstm",
    "long short-term memory",
    "expression graphs",
    "automatic differentiation",
    "backprop",
    "backpropagation"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "prepare": "npm run test",
